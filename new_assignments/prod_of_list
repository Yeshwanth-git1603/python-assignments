# prod of the elements in the list
from functools import reduce
def prod_list(list1):
    s=1
    for x in list1:
        s=s*x
    print(s)
    list2=[]
    list2.append(s)
    print("the prod of the list elements is:",list2)
list1=[1,2,3,4,5]
prod_list(list1)

res=reduce(lambda x,y:x*y,list1)
print("the prod of the list is:",res)
